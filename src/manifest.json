{
  "manifest_version": 3,
  "name": "Scrollbar for mobile",
  "author": "TeJAVA",
  "version": "0.1",
  "default_locale": "en",
  "description": "Adds a movable scroll bar for better navigation on mobile devices.",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_url": "manifest.webmanifest"
}